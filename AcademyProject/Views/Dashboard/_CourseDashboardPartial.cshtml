@model List<UserViewModel>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Duration</th>
            <th scope="col">Price</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tbody>
            <tr>
                <td>@item.CourseName</td>
                <td>@item.CourseDuration</td>
                <td>@item.CoursePrice</td>
            </tr>
        </tbody>
    }
</table>